{
    "name": "borodulin/presenter-bundle",
    "description": "Presenter Bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrey Borodulin",
            "email": "borodulin@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "symfony/http-kernel": "^5.4",
        "symfony/property-access": "^5.4",
        "symfony/property-info": "^5.4",
        "symfony/serializer": "^5.4",
        "symfony/http-foundation": "^5.4",
        "symfony/yaml": "^5.4",
        "doctrine/orm": "^2.7",
        "symfony/validator": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/framework-bundle": "^5.4",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Borodulin\\PresenterBundle\\": "src/"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^5.4"
        }
    }
}
